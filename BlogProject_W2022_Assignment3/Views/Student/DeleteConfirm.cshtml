@model BlogProject.Models.Student
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this teacher?</h2>

<div>First Name: @Model.StudentFName</div>
<div>Last Name: @Model.StudentLName</div>

<form method="POST" action="/Studnet/Delete/@Model.StudentId">
    <input type="submit" value="Delete" />

</form>