@model BlogProject.Models.Teacher
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this teacher?</h2>

<div>First Name: @Model.TeacherFName</div>
<div>Last Name: @Model.TeacherLName</div>

<form method="POST" action="/Teacher/Delete/@Model.TeacherId">
    <input type="submit" value="Delete"/>

</form>